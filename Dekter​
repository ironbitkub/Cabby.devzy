local ScreenGui = Instance.new("ScreenGui", game.Players.LocalPlayer:WaitForChild("PlayerGui"))
local MainFrame = Instance.new("Frame", ScreenGui)
MainFrame.Size = UDim2.new(0, 400, 0, 300)
MainFrame.Position = UDim2.new(0.5, -200, 0.5, -150)
MainFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)

local TabButtons = Instance.new("Frame", MainFrame)
TabButtons.Size = UDim2.new(1, 0, 0, 30)
TabButtons.BackgroundColor3 = Color3.fromRGB(50, 50, 50)

local ContentFrame = Instance.new("Frame", MainFrame)
ContentFrame.Size = UDim2.new(1, 0, 1, -30)
ContentFrame.Position = UDim2.new(0, 0, 0, 30)
ContentFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)

local function createTab(name)
    local btn = Instance.new("TextButton", TabButtons)
    btn.Text = name
    btn.Size = UDim2.new(0, 100, 1, 0)
    btn.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
    btn.TextColor3 = Color3.new(1, 1, 1)
    local frame = Instance.new("Frame", ContentFrame)
    frame.Size = UDim2.new(1, 0, 1, 0)
    frame.Visible = false
    frame.BackgroundTransparency = 1
    btn.MouseButton1Click:Connect(function()
        for _, f in pairs(ContentFrame:GetChildren()) do f.Visible = false end
        frame.Visible = true
    end)
    return frame
end

local mainTab = createTab("Main")
local playerTab = createTab("Player")
local musicTab = createTab("Music")
local teleportTab = createTab("Teleport")

mainTab.Visible = true

local function createButton(tab, text, callback)
    local btn = Instance.new("TextButton", tab)
    btn.Size = UDim2.new(0, 200, 0, 30)
    btn.Position = UDim2.new(0, 10, 0, #tab:GetChildren() * 35)
    btn.Text = text
    btn.BackgroundColor3 = Color3.fromRGB(90, 90, 90)
    btn.TextColor3 = Color3.new(1, 1, 1)
    btn.MouseButton1Click:Connect(callback)
end

createButton(mainTab, "Bring Mob", function() end)
createButton(mainTab, "Kill Aura", function() end)
createButton(mainTab, "TP to Camp", function()
    local plr = game.Players.LocalPlayer
    if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
        plr.Character.HumanoidRootPart.CFrame = CFrame.new(0, 10, 0)
    end
end)
createButton(mainTab, "Body Light", function()
    local plr = game.Players.LocalPlayer
    local char = plr.Character or plr.CharacterAdded:Wait()
    local hrp = char:WaitForChild("HumanoidRootPart")
    local light = hrp:FindFirstChild("BodyLight")
    if light then
        light:Destroy()
    else
        local newLight = Instance.new("PointLight")
        newLight.Name = "BodyLight"
        newLight.Parent = hrp
        newLight.Range = 200
        newLight.Brightness = 10
        newLight.Color = Color3.new(1, 1, 1)
    end
end)

createButton(playerTab, "WalkSpeed 100", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 100
end)
createButton(playerTab, "JumpPower 100", function()
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = 100
end)
createButton(playerTab, "Infinite Jump", function()
    local plr = game.Players.LocalPlayer
    game:GetService("UserInputService").JumpRequest:Connect(function()
        plr.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
    end)
end)

createButton(musicTab, "ESP Player", function()
    for _, v in pairs(game.Players:GetPlayers()) do
        if v ~= game.Players.LocalPlayer and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
            local billboard = Instance.new("BillboardGui", v.Character.HumanoidRootPart)
            billboard.Size = UDim2.new(0, 100, 0, 50)
            billboard.AlwaysOnTop = true
            local label = Instance.new("TextLabel", billboard)
            label.Size = UDim2.new(1, 0, 1, 0)
            label.Text = v.Name
            label.BackgroundTransparency = 1
            label.TextColor3 = Color3.fromRGB(0, 255, 255)
            local line = Instance.new("Beam", v.Character.HumanoidRootPart)
            local att0 = Instance.new("Attachment", game.Players.LocalPlayer.Character.HumanoidRootPart)
            local att1 = Instance.new("Attachment", v.Character.HumanoidRootPart)
            line.Attachment0 = att0
            line.Attachment1 = att1
            line.Color = ColorSequence.new(Color3.fromRGB(0, 255, 255))
            line.Width0 = 0.1
            line.Width1 = 0.1
        end
    end
end)
createButton(musicTab, "ESP Item", function() end)

local tpList = Instance.new("ScrollingFrame", teleportTab)
tpList.Size = UDim2.new(0, 200, 0, 200)
tpList.Position = UDim2.new(0, 10, 0, 10)
tpList.CanvasSize = UDim2.new(0, 0, 0, 0)

local updateBtn = Instance.new("TextButton", teleportTab)
updateBtn.Size = UDim2.new(0, 200, 0, 30)
updateBtn.Position = UDim2.new(0, 10, 0, 220)
updateBtn.Text = "Update List"
updateBtn.BackgroundColor3 = Color3.fromRGB(90, 90, 90)
updateBtn.TextColor3 = Color3.new(1, 1, 1)

updateBtn.MouseButton1Click:Connect(function()
    for _, v in pairs(tpList:GetChildren()) do if v:IsA("TextButton") then v:Destroy() end end
    local y = 0
    for _, v in pairs(game.Players:GetPlayers()) do
        if v ~= game.Players.LocalPlayer then
            local tpBtn = Instance.new("TextButton", tpList)
            tpBtn.Size = UDim2.new(1, 0, 0, 30)
            tpBtn.Position = UDim2.new(0, 0, 0, y)
            tpBtn.Text = v.Name
            tpBtn.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
            tpBtn.TextColor3 = Color3.new(1, 1, 1)
            tpBtn.MouseButton1Click:Connect(function()
                local plr = game.Players.LocalPlayer
                if plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") and v.Character and v.Character:FindFirstChild("HumanoidRootPart") then
                    plr.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame
                end
            end)
            y = y + 35
            tpList.CanvasSize = UDim2.new(0, 0, 0, y)
        end
    end
end)
